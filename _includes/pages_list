	{% if group == null or group == node.group and node.title != "Home" %}
		{% if page.url == node.url %}
			<li class="active"><a href="{{ node.url }}">{{ node.title }}</a></li>
		{% else %}
			<li><a href="{{ node.url }}">{{ node.title }}</a></li>
		{% endif %}
	{% endif %}
{% endfor %}
{% assign pages_list = nil %}
{% assign group = nil %}